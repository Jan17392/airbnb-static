<div class="container text-center">
  <h1>Our Team</h1>

  <ul class="list-inline">
    <% ["anne", "romain", "seb"].each do |buddy| %>
      <li>
        <h3> <%= buddy.capitalize %> </h3>
        <%= image_tag "#{buddy}.jpg", class: "img-circle image-medium" %>
      </li>
    <% end %>
  </ul>
  <h3 class="padded">
    <a href="mailto:contact@airbnb.com">
      <i class="fa fa-envelope"></i> contact@airbnb.com
    </a>
  </h3>
</div>
